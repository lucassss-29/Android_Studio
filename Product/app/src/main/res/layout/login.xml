<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/textViewEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="E-mail"
        android:textStyle="normal"
        android:visibility="visible"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="256dp" />

    <TextView
        android:id="@+id/textViewPassword"
        style="Sofia Pro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Password"
        android:visibility="visible"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="331dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="170dp"
        android:layout_height="71dp"
        android:text="FACEBOOK"
        android:textColor="#000000"
        android:textSize="16dp"
        app:backgroundTint="#FFFFFF"
        app:cornerRadius="30dp"
        app:icon="@drawable/group_17863"
        app:iconSize="35dp"
        app:iconTint="#FFFFFF"
        app:iconTintMode="multiply"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="632dp" />

    <TextView
        android:id="@+id/textViewLogin"
        style="Sofia Pro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Login"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#000000"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="193dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="170dp"
        android:layout_height="71dp"
        android:text="GOOGLE"
        android:textColor="#000000"
        android:textSize="16dp"
        app:backgroundTint="#FFFFFF"
        app:cornerRadius="30dp"
        app:icon="@drawable/group_17967"
        app:iconSize="35dp"
        app:iconTint="#FFFFFF"
        app:iconTintMode="multiply"
        tools:layout_editor_absoluteX="225dp"
        tools:layout_editor_absoluteY="632dp" />


    <TextView
        android:id="@+id/textViewForgot"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Forgot password?"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#FE724C"
        android:textSize="15dp"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="401dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Don't have an account ?"
        android:textSize="12dp"
        tools:layout_editor_absoluteX="122dp"
        tools:layout_editor_absoluteY="523dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sign Up"
        android:textColor="#FE724C"
        android:textSize="12dp"
        tools:layout_editor_absoluteX="255dp"
        tools:layout_editor_absoluteY="523dp" />

    <TextView
        android:id="@+id/textViewGoogle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sign in with"
        android:textSize="12dp"
        tools:layout_editor_absoluteX="174dp"
        tools:layout_editor_absoluteY="598dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="296dp"
        android:layout_height="167dp"
        android:layout_weight="0.8"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/login1"
        tools:layout_editor_absoluteX="-41dp"
        tools:layout_editor_absoluteY="-83dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="179dp"
        android:layout_height="200dp"
        app:srcCompat="@drawable/login3"
        tools:layout_editor_absoluteX="297dp"
        tools:layout_editor_absoluteY="-93dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="134dp"
        android:layout_height="92dp"
        android:layout_weight="0.2"
        app:srcCompat="@drawable/login2"
        tools:layout_editor_absoluteX="-51dp"
        tools:layout_editor_absoluteY="-21dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="319dp"
        android:layout_height="69dp"
        android:text="Login"
        android:textSize="20dp"
        app:backgroundTint="#FE724C"
        app:cornerRadius="30dp"
        tools:layout_editor_absoluteX="52dp"
        tools:layout_editor_absoluteY="442dp" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="343dp" />

    <EditText
        android:id="@+id/editTextTextPassword2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="275dp" />


</androidx.constraintlayout.widget.ConstraintLayout>